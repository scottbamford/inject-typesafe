  "module": "dist/lib/esm/index.js",
  "types": "dist/lib/esm/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Scott Bamford",
    "email": "scott@ambidect.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbamford/inject-typesafe.git"
  },
  "keywords": [
    "dependency-injection",
    "type-safe",
    "dependency",
    "inject",
    "typesafe",
    "typescript",
    "IoC",
    "di"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/scottbamford/inject-typesafe/issues"
  },
  "homepage": "https://github.com/scottbamford/inject-typesafe/#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc && tsc -m esnext --outDir ./dist/lib/esm"
